{
    "name": "@rat.md/rollup-plugin-sass",
    "version": "0.4.0",
    "status": "beta",
    "description": "A SASS / SCSS rollup plugin, especially designed for the development of our rat packages.",
    "keywords": [
        "rollup-plugin",
        "rollup",
        "sass",
        "scss",
        "css",
        "rat"
    ],
    "author": "SamBrishes <sam@rat.md> (https://www.rat.md)",
    "license": "MIT",
    "copyright": "rat.md <info@rat.md>",
    "homepage": "https://rat.md/rollup-plugin-sass",
    "bugs": {
        "url": "https://github.com/RatMD/rollup-plugin-sass/issues",
        "email": "info@rat.md"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com:RatMD/rollup-plugin-sass.git"
    },
    "funding": [
        {
            "type": "buymeacoffee",
            "url": "https://buymeacoff.ee/ratMD"
        },
        {
            "type": "paypal",
            "url": "https://paypal.me/pytesNET"
        }
    ],
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c --watch",
        "prepare": "rollup -c"
    },
    "type": "module",
    "main": "dist/rollup-sass.js",
    "module": "dist/rollup-sass.mjs",
    "exports": {
        "import": "./dist/rollup-sass.mjs",
        "require": "./dist/rollup-sass.js"
    },
    "files": [
        "dist/*.js",
        "dist/*.mjs",
        "src/types/*d.ts"
    ],
    "typings": "src/types/ratsass.d.ts",
    "reveal": true,
    "dependencies": {
        "@rollup/pluginutils": "^5.0.2",
        "sass": "^1.63.6"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.1.2",
        "@types/node": "^18.16.0",
        "rollup": "^3.25.3",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6"
    }
}
